<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ExerciseResultActivity">

    <TextView
        android:id="@+id/tvExerciseTime"
        android:layout_width="128dp"
        android:layout_height="18dp"
        android:layout_marginTop="92dp"
        android:layout_marginEnd="61dp"
        android:layout_marginBottom="117dp"
        android:text="운동시간 : "
        app:layout_constraintBottom_toTopOf="@+id/tvExerciseStep"
        app:layout_constraintEnd_toEndOf="@+id/textView6"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tvExerciseDistance"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="20dp"
        android:text="운동거리 :"
        app:layout_constraintBottom_toTopOf="@+id/tvExerciseCalorie"
        app:layout_constraintStart_toStartOf="@+id/tvExerciseTime"
        app:layout_constraintTop_toBottomOf="@+id/tvExerciseTime" />

    <TextView
        android:id="@+id/tvExerciseCalorie"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="322dp"
        android:text="소모 칼로리 :"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/tvExerciseDistance"
        app:layout_constraintTop_toBottomOf="@+id/tvExerciseDistance" />

    <TextView
        android:id="@+id/tvExerciseStep"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="265dp"
        android:text="걸음수 :"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/tvExerciseCalorie"
        app:layout_constraintTop_toBottomOf="@+id/tvExerciseTime" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="76dp"
        android:layout_height="0dp"
        android:layout_marginTop="29dp"
        android:layout_marginBottom="217dp"
        android:text="운동 종료"
        app:layout_constraintBottom_toTopOf="@+id/tvExerciseDate"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tvExerciseDate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="216dp"
        android:text="운동날짜 :"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/tvExerciseStep"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />
</androidx.constraintlayout.widget.ConstraintLayout>